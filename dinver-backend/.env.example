# Dinver Backend Environment Variables

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/dinver_db

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# OpenAI API (for intent classification and old LLM)
OPENAI_API_KEY=sk-...

# Anthropic Claude API (for new enhanced AI responses)
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-...
# Alternative env var name (both work)
CLAUDE_API_KEY=sk-ant-...

# AWS S3
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=eu-central-1
S3_BUCKET_NAME=dinver-media

# CloudFront CDN
CLOUDFRONT_DOMAIN=d1234567890.cloudfront.net
CLOUDFRONT_KEY_PAIR_ID=your_key_pair_id
CLOUDFRONT_PRIVATE_KEY=path/to/private-key.pem

# Google APIs
GOOGLE_PLACES_API_KEY=your_google_places_key
GOOGLE_CLOUD_PROJECT_ID=your_project_id

# Redis
REDIS_URL=redis://localhost:6379

# Server
PORT=5000
NODE_ENV=development

# AI Configuration
AI_MODEL=claude-sonnet-4-20250514  # or gpt-4o-mini for cheaper option
AI_TEMPERATURE=0.3
AI_MAX_TOKENS=1024

# Performance
AI_CACHE_TTL=600  # 10 minutes
TYPES_CACHE_TTL=3600  # 1 hour

# Feature Flags
POSTGRES_SIMILARITY_ENABLED=false  # Enable after pgvector setup
USE_CLAUDE_AI=true  # Set to false to use OpenAI instead
