{
  "expo": {
    "name": "Dinver",
    "slug": "dinver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dinver",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": [
      {
        "asset": "assets/fonts/Degular-Regular.otf",
        "family": "Degular"
      },
      {
        "asset": "assets/fonts/Degular-Medium.otf",
        "family": "Degular-Medium"
      },
      {
        "asset": "assets/fonts/Degular-Bold.otf",
        "family": "Degular-Bold"
      },
      {
        "asset": "assets/fonts/Degular-Semibold.otf",
        "family": "Degular-Semibold"
      },
      {
        "asset": "assets/fonts/Onest-Regular.ttf",
        "family": "Onest"
      },
      {
        "asset": "assets/fonts/Onest-Medium.ttf",
        "family": "Onest-Medium"
      },
      {
        "asset": "assets/fonts/Onest-Bold.ttf",
        "family": "Onest-Bold"
      }
    ]
  }
}
